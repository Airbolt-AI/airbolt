{
  "name": "@airbolt/core",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "import": "./dist/services/*.js"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "import": "./dist/plugins/*.js"
    },
    "./routes/*": {
      "types": "./dist/routes/*.d.ts",
      "import": "./dist/routes/*.js"
    },
    "./routes/api/*": {
      "types": "./dist/routes/api/*.d.ts",
      "import": "./dist/routes/api/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "dev": "tsc -p tsconfig.build.json --watch",
    "lint": "eslint .",
    "test": "cd ../.. && NODE_OPTIONS='--import tsx' pnpm exec vitest run packages/core/test/**/*.test.ts",
    "test:watch": "cd ../.. && NODE_OPTIONS='--import tsx' pnpm exec vitest packages/core/test/**/*.test.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/openai": "^1.3.23",
    "@airbolt/auth": "workspace:*",
    "@airbolt/config": "workspace:*",
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "ai": "^4.3.19",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "fastify-sse-v2": "^4.2.1",
    "rate-limiter-flexible": "^7.1.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@airbolt/test-utils": "workspace:*",
    "@types/node": "^22.10.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "fastify": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}