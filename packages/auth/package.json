    "lint": "eslint ."
  },
  "dependencies": {
    "@airbolt/config": "workspace:*",
    "@fastify/jwt": "^9.1.0",
    "@fastify/sensible": "^6.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "fastify": "^5.2.2",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7"
  },
  "devDependencies": {
    "@airbolt/test-utils": "workspace:*",
    "@fast-check/vitest": "^0.2.2",
    "@types/node": "^22.10.6",
    "fast-check": "^4.1.1",
    "vitest": "^3.2.3"
  }
}
